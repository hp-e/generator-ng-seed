    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.12.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.0.9",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.16.0"
  },
  "dependencies": {
